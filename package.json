{
  "name": "fptsbackend",
  "version": "0.1.0-alpha.1",
  "description": "functional ts backend examples",
  "author": "ondraba",
  "license": "ISC",
  "homepage": "https://",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && concurrently \"tsc --watch\" \"cross-env NODE_ENV=DEVELOPMENT nodemon\"",
    "debug": "set NODE_ENV=DEVELOPMENT&& npm run build && node %NODE_DEBUG_OPTION% dist/index.js\"",
    "prod": "npm run build && concurrently \"tsc --watch\" \"nodemon\"",
    "start": "node dist/index.js",
    "stop": "node stop dist/index.js",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint:check": "tslint-config-prettier-check ./tslint.json",
    "test": "jest --verbose --colors --runInBand --no-cache",
    "prettier:show": "prettier --list-different \"./**/*\"",
    "prettier:write": "prettier \"./**/*\" --write",
    "precommit": "pretty-quick --staged && npm run tslint",
    "coverage": "jest --coverage --runInBand"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(src/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "dependencies": {
    "axios": "^0.21.0",
    "dataloader": "^2.0.0",
    "dotenv": "6.2.0",
    "express": "4.17.1",
    "fp-ts": "^2.9.3",
    "fp-ts-contrib": "^0.1.21",
    "http-status-codes": "^1.4.0",
    "hyper-ts": "^0.6.0",
    "io-ts": "^2.2.13",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.3",
    "monocle-ts": "^2.3.4",
    "newtype-ts": "^0.3.4",
    "typescript-logging": "^0.6.3",
    "uuid": "^3.3.3",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@types/express": "4.17.9",
    "@types/jest": "26.0.19",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mongodb": "^3.6.3",
    "@types/node": "14.14.14",
    "@types/uuid": "^3.4.5",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "husky": "1.2.1",
    "io-ts-types": "^0.5.12",
    "jest": "26.6.3",
    "nodemon": "1.18.9",
    "prettier": "1.15.3",
    "pretty-quick": "1.8.0",
    "rimraf": "2.6.2",
    "ts-jest": "23.10.5",
    "tslint": "5.12.0",
    "tslint-config-prettier": "1.17.0",
    "typescript": "4.1.3"
  }
}
